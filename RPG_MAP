NEXT:NEXT
ENDIF

ELSE

E=0
ENDIF


IF F==1 THEN BEEP 2,,SEVO
F=0

UNTIL BUTA==32:BEEP 4,,SEVO

'CONCLS
CONCLS 14,0,34,22
WAIT 2
C=0:D=0:E=0:F=0
MOSPD=GENERA1[0]
'DASHC=GENERA1[1]
'WAIT 10
GOTO @MENEW_1

@SAVED

END

'繧､繝吶Φ繝医＞縺｡繧峨ｓ=======
@EVENT_
EVENT_F=EVE
'END
'?CTALKT

'IF EVE>=1&&EVE!=9 THEN 
'TAKL5 EVE-1,EVE-1,0:EVENTEND EVE-1,1

'IF EVE>=16&&EVE<=18 THEN 
IF MAP==0 THEN 
IF EVE==9 THEN 
TAKL5 8,8,0
TAKL5 8,17,2
TAKL5 8,18,3
TALKOPEN 
EVENTOPEN 8,1
MOVE 8,"3333223?????????????????????????????????????????????????????LSEIF EVE>=16&&EVE<=18 THEN

MAP=1:LOCAMAPX=11:LOCAMAPY=4:FHOUKOU=1:BGCLR:SPCLR:EVENTEND EVE-1,0
GOTO @MAPLOAD


ELSE 

TAKL5 EVE-1,EVE-1,0:EVENTEND EVE-1,1

ENDIF

ENDIF

IF MAP==1 THEN 'EVENT_F=0

IF EVE>=2&&EVE<=3 THEN 
MAP=0:LOCAMAPX=20:LOCAMAPY=47:FHOUKOU=0:BGCLR:SPCLR:EVENTEND EVE-1,0
GOTO @MAPLOAD
ENDIF
IF EVE==4 THEN TAKL5 3,0,0:EVENTOPEN 3,1:MOVE 3,"00500",2:EVENTEND 3,3
'###
IF EVE==5 THEN TAKL5 4,1,0:EVENTEND 4,1
IF EVE==6 THEN TAKL5 5,2,0:EVENTEND 5,1

ENDIF

RETURN

'CTIMES[=0
DEF EVENTEND SPHEN,ENDNMB
IF CTIMES[SPHEN]==ENDNMB THEN EVENT_F=0:EVE=0:CTIMES[SPHEN]=0
END

DEF EVENTOPEN SPHEN,ENDNMB
IF CTIMES[SPHEN]==ENDNMB THEN EVENT_F=0:EVE=0:CTIMES[SPHEN]=ENDNMB+1
END
CTALKT=0
DEF TALKOPEN 'SPHEN,ENDNMB
IF CTALKT==1 THEN EVENT_F=0:EVE=0:CTALKT=0
END

MOVEQ$=""
DEF MOVE SPHEN,MOVEQ$,TIMEOK
IF CTIMES[SPHEN]==TIMEOK THEN EVENTS$[SPHEN]=MOVEQ$':EVENTV[SPHEN]=0
END
'A=縺ｶ繧薙＠繧・≧ 縺ｯ縺､縺ｩ縺・§繧・≧縺代ｓ
'DEF TAKL5 SPHEN,A,RCTIMES'CCT$












'@MAPDATA_0_1
'繧ゅ§縺ｦ縺・℃=================
'DATA ""'MAP
'DATA ""
'DATA -1


FIRST0=0:CCUNT=0:WCOUNT1=0:PGE=0:DSEJ=0
GOHI=0:CCUNT1=0:HIWID=0:HIWWW=0
XDIX=0:YDIX=0:WAITFB=0
FIRST11=0:FIRST10=0
CTALKT=0
DEF TAKL5 SPHEN,A,RCTIMES'CCT$
IF CTIMES[SPHEN]==RCTIMES THEN
IF CTALKT==0 THEN 
CCT$=CCTT$[A]
IF FIRST11==1 THEN CCT$=CCTT$[GOHI]

IF FIRST0==0 THEN 
HIWID=2
CONBOX 1,1+11*HIWID,48,6+11*HIWID
XDIX=0''+11*HIWID
YDIX=1+11*HIWID+1
FIRST0=1

ENDIF
CCTA$=CCT$[CCUNT]
'*/縺倥°繧薙ｒ縺ｾ縺､===========/*
INC WCOUNT1
IF WCOUNT1>=WAITFB THEN 
   WCOUNT1=0:WAITFB=3*MOSPD
ELSE 
   CCTA$=""
   DEC CCUNT
ENDIF
'*/==============/*

'*/繝壹・繧ｸ繧偵°縺医ｋ==================/*
IF CCTA$=="誧・ THEN
CCTA$="ﾎ・
IF PGE==2 THEN CCTA$="^"
PGE=1
ENDIF
'*/======================/*

'*/縺ｨ縺上∋縺､縺ｾ縺､==================/*
IF CCTA$=="@" THEN
   WAITFB=10
   CCTA$=""
ENDIF
'IF CCTA$=="{" THEN
'   WAITFB=
'   CCTA$=""
'ENDIF
'*/======================/*


'*/A繝懊ち繝ｳ繧偵∪縺､==================/*
IF CCTA$=="ﾎ・ THEN
   CCTA$=""
   IF BUTA!=16 THEN 
      DEC CCUNT
   ELSE 
      BEEP 3,,SEVO
      IF ?????????????????????????????????????????????????????ENDIF
ENDIF
'*/======================/*


'*/繝壹・繧ｸ繧偵￠縺・=================/*

IF CCTA$=="^" THEN
'縺代☆繧√＞繧後＞
'CONCLS 0,0,20,0
FOR A=0 TO 3
LOCATE 2,2+11*HIWID+A:?" "*46
NEXT
'DEC CCUNT
CCTA$=""
CCUNT1=0
IF PGE==2 THEN PGE=0
ENDIF

'*/======================/*


'*/YES-NO=================/*
IF CCTA$=="/" THEN
   IF FIRST10==0 THEN 
      IF HIWID==2 THEN HIWWW=10
         CONBOX 1,YDIX+5-HIWWW,6,YDIX+8-HIWWW
   FIRST10=1
   ENDIF
CCTA$=""
XY 2,YDIX+6-HIWWW',5,10+11*HIWID
DSEJ=SELECT(2,DSEJ)'縺帙ｓ縺溘￥縺倥＆縺上ａ縺・ｌ縺・LODI 3,YDIX+6-HIWWW,"縺ｯ縺・
LODI 3,YDIX+7-HIWWW,"縺・＞縺・
IF BUTA AND 16 THEN ':LOCATE 5,5:?
CONCLS 1,YDIX+5-HIWWW,6,YDIX+8-HIWWW:FIRST10=0
HIWWW=0
GOHI=VAL(MID$(CCT$,CCUNT+1+2*DSEJ,2)):FIRST11=1
CCT$=LEFT$(CCT$,LEN(CCT$)-4)
ELSE 
DEC CCUNT
ENDIF
ENDIF
'*/======================/*


'*/縺輔＞縺励ｅ縺・ND=================/*
IF CCTA$=="\" THEN
CCTA$=""
INC CTIMES[SPHEN]
CONCLS 1,1+11*HIWID,48,6+11*HIWID
FIRST0=0
FIRST10=0
FIRST11=0
CCUNT1=0
'CTALKT=1
ENDIF
IF CCTA$=="~" THEN
CCTA$=""
CONCLS 1,1+11*HIWID,48,6+11*HIWID
FIRST0=0
FIRST10=0
FIRST11=0
CCUNT1=0
CTALKT=1
ENDIF
'*/======================/*


LOCATE (XDIX+CCUNT1) MOD 46+2,YDIX+FLOOR((XDIX+CCUNT1)/46)
?CCTA$
IF CCTA$!="" THEN INC CCUNT1:BEEP 9,,SEVO
INC CCUNT
IF CCUNT>=LEN(CCT$) THEN CCUNT=0'INC CTIMES

ENDIF
ENDIF
END
