   INC ITAB,1*(PTABLE[1,D]>0)*F
   NEXT
   '縺輔＞縺縺・・繝ｼ繧ｸ
   CB=FLOOR(ITAB/21)
   IF (ITAB MOD 21)==0 THEN DEC CB
   CA=FLOOR(AD/21)
   AA=AD MOD 21':AA=0
 '縺励◆繧・'?ITAB:END
RETURN

















