@ITEM_KATU
 '繧｢繧､繝・Β繧偵＞縺｣縺九▽縺吶ｋ繧・   E=0
   FOR D=0 TO ITAB-1
   
   IF PTABLE[0,D]==A&&(PTABLE[3,D]==-1||AD==D) THEN 
      INC E,PTABLE[1,D]:PTABLE[1,D]=-1
    IF AD!=D THEN PTABLE[0,D]=-1
   ENDIF
   
   NEXT
   PTABLE[1,AD]=E
   IF E>99 THEN 
    '繧｢繧､繝・Β繧偵″縺帙＞縺九☆繧九ｈ
      FOR H=1 TO CEIL(E/99)-1
       F=PTABLE[0,AD+H]
       G=PTABLE[1,AD+H]
       I=PTABLE[2,AD+H]
       J=PTABLE[3,AD+H]
         FOR D=AD+H TO ITAB
'FOR R=0 TO 20

'LOCATE 30,1+R:?PTABLE[3,R]
'NEXT
'WAIT 
          SWAP F,PTABLE[0,D+1]
          SWAP G,PTABLE[1,D+1]
          SWAP I,PTABLE[2,D+1]
          SWAP J,PTABLE[3,D+1]
         NEXT
       PTABLE[0,AD+H-1]=PTABLE[0,AD]
'       PTABLE[3,AD+H-1]=PTABLE[3,AD]
       PTABLE[1,AD+H-1]=E-99*(H-1)
       IF H>1 THEN PTABLE[3,AD+H-1]=-1
         IF E-99*(H-1)>=99 THEN 
          PTABLE[1,AD+H-1]=99
         ENDIF
       INC ITAB
      NEXT
    PTABLE[0,AD+H-1]=PTABLE[0,AD]
    IF H>1 THEN PTABLE[3,AD+H-1]=-1'PTABLE[3,AD]
    PTABLE[1,AD+H-1]=E-99*(H-1)
    IF E-99*(H-1)>=99 THEN 
    PTABLE[1,AD+H-1]=99
    ENDIF
   ENDIF



 '繧｢繧､繝・Β縺ｮ繧後▽繧偵○縺・ｊ縺吶ｋ繧・   E=0
   FOR D=0 TO ITAB-1
   IF PTABLE[1,D]>0 THEN 
      PTABLE[0,E]=PTABLE[0,D]
      PTABLE[1,E]=PTABLE[1,D]
      PTABLE[2,E]=PTABLE[2,D]
      PTABLE[3,E]=PTABLE[3,D]
      IF AD==D THEN AD=E
     IF D!=E THEN 
      PTABLE[0,D]=-1
      PTABLE[1,D]=-1
      PTABLE[2,D]=-1
      PTABLE[3,D]=-1
     ENDIF
      INC E
   ENDIF
   NEXT
 '縺励◆繧・

FOR D=0 TO MPOUCH-1

IF POUCH[D]!=-1 THEN 
E=-1:REPEAT:INC E:UNTIL PTABLE[3,E]==10+D
POUCH[D]=E
ENDIF
NEXT


GOSUB @ITEM_PGCL


RETURN

@ITEM_PGCL
 '縺輔＞縺縺・■繧偵＆縺・※縺阪°縺吶ｋ繧・   E=127:ITAB=0
'?E:END
   'ITEM縺ｮ縺薙☆縺・   FOR D=0 TO E-1
   F=1
   IF PTABLE[0,D]==-1 THEN BREAK
